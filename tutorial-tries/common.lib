# common.lib
# Note no #!/usr/bin/sh as this should not spawn an extra shell. It's
# end of the world to have, but clearer not to.
#
STD_MSG="About to rename some files"
rename()
{
      FROM=$1
      TO=$2

      for i in *$FROM
      do
           j=`basename $i $FROM`
           mv $i ${j}$TO 
      done
}           
